    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "form-data": "^3.0.0",
    "migrate-mongo": "^7.0.1",
    "mongodb": "^3.5.2",
    "mongodb-migrations": "^0.8.5",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mustache": "^4.0.0",
    "nodemailer": "^6.4.2",
